source 'https://rubygems.org'

gem 'rails', '3.2.6'

# Postgresql Adapter
gem 'pg'

# Redis
gem 'redis'

# 7digital
gem '7digital'

# Will paginate
gem 'will_paginate'

# Gems used only for Development
group :development do
	gem 'annotate'
end

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'

  gem 'uglifier', '>= 1.0.3'
end

gem 'jquery-rails'
