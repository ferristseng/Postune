<div class="row">
	<div class="span8">
		<div class="left">
			<%= station_label @station %>
			<%= link_to_favorite @station %>
			<span class="label"><%= @station.name %></span>
		</div>
		<div class="right text">
			<div class="counter"><span id="online-users">0</span> <span class="icon-user"></span></div>
			<div class="counter"><span id="favorite-count"><%= @station.favorites_count %></span> <span class="icon-heart"></span></div>
		</div>
	</div>
</div>